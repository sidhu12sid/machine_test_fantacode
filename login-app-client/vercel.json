{
  "version": 2,
  "builds": [
    {
      "src": "login-app-client/dist/login-app-client/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "login-app-client/dist/login-app-client/browser/index.html"
    }
  ]
}
